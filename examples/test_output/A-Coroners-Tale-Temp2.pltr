      "bookId": 1,
      "positionWithinLine": 8,
      "positionInBeat": 0,
      "title": "Edwin receives a message",
      "description": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Edwin receives a message from someone who says he knows about Edwin’s past and they need to meet."
            }
          ]
        }
      ],
