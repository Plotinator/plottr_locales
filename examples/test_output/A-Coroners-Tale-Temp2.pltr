      "beatId": 1,
      "bookId": 1,
      "positionWithinLine": 0,
      "positionInBeat": 0,
      "title": "Sarah tells Edwin she told Ralf about them",
      "description": [
        {
          "type": "paragraph",
          "children": [
            {
              "children": [
                {
                  "text": "Sarah tells Edwin she’s admitted their relationship to Ralf and he’s promised to keep it secret."
                }
              ]
            }
          ]
        }
      ],
      "tags": [],
      "characters": [],
      "places": [],
