    {
      "id": 18,
      "lineId": 3,
      "beatId": 1,
      "bookId": 1,
      "positionWithinLine": 0,
      "positionInBeat": 0,
      "title": "Meg follows Geoffrey",
      "description": [
        {
          "type": "paragraph",
          "children": [
            {
              "children": [
                {
                  "text": "Meg follows Geoffrey to a tavern and sees him drunk and with a woman. He sees them go upstairs."
                }
              ]
            }
          ]
        }
      ],
