    {
      "id": 13,
      "lineId": 2,
      "beatId": 1,
      "bookId": 1,
      "positionWithinLine": 0,
      "positionInBeat": 0,
      "title": "Edwin talks to thief-taker Zeb Garbutt",
      "description": [
        {
          "type": "paragraph",
          "children": [
            {
              "children": [
                {
                  "text": "Edwin hears that Sarah has approached another thief-taker, someone less scrupulous."
                }
              ]
            }
          ]
        }
      ],
      "tags": [],
      "characters": [],
      "places": [],
      "templates": [],
