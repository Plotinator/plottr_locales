    },
    {
      "id": 49,
      "lineId": 4,
      "beatId": 1,
      "bookId": 1,
      "positionWithinLine": 0,
      "positionInBeat": 0,
      "title": "Edwin is introduced to Ralf",
      "description": [
        {
          "type": "paragraph",
          "children": [
            {
              "children": [
                {
                  "text": "Edwin is introduced to Ralf by Sarah, when they meet one day when walking. Edwin notices how close they are."
                }
              ]
            }
          ]
        }
      ],
      "tags": [],
      "characters": [],
      "places": [],
      "templates": [],
      "imageId": null,
