    {
      "id": 4,
      "title": "Notes on pride",
      "content": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Note: Notes on pride"
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "pride, noun: "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "1: the quality or state of being proud: such as "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "a: inordinate self-esteem : CONCEIT "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "b: a reasonable or justifiable self-respect "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "c: delight or elation arising from some act, possession, or relationship "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "parental pride "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "2: proud or disdainful behavior or treatment : DISDAIN "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Source: Merriam-Webster"
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "..."
            }
          ]
        }
      ],
